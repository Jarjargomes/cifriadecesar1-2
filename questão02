		System.out.println("Digite aqui o seu c√≥digo");
		String codigos = sc.nextLine();
		ArrayList<Integer> d = new ArrayList<Integer>();
		ArrayList<Integer> e = new ArrayList<Integer>();
		ArrayList<Integer> f = new ArrayList<Integer>();

		for (int i = 0; i < codigos.length(); i++) {
			String cesa = codigos.substring(i, i + 1);
			int x = Integer.parseInt(cesa);
			d.add(x);
		}
		for (int g = 0; g < codigos.length(); g++) {
			if (d % 2 == 0) {
				e.add((d.get(g)));
			} else {
				f.add((d.get(g)));
			}
		}
		for (int h = 0; h < f.size(); h++) {
			texto.add((matriz[e.get(h)][f.get(h)]));
		}
		System.out.println(segundaq);
	}

}
