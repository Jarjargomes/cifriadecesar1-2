	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		ArrayList<String> segundaq = new ArrayList<String>();
		String matriz[][] = new String[6][6];
		matriz[0][0] = " ";
		matriz[0][1] = " ";
		matriz[0][2] = " ";
		matriz[0][3] = " ";
		matriz[0][4] = " ";
		matriz[0][5] = " ";
